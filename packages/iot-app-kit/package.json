{
  "name": "@iot-app-kit/core",
  "version": "0.0.1",
  "description": "IoT Application Kit core",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/interface.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/bp-components/bp-components.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf screenshot",
    "build": "npm-run-all build:stencil",
    "build:stencil": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "npm-run-all -p test:unit-test lint test:typescript",
    "test:dev": "npm-run-all test test:integ",
    "test:unit-test": "TZ=UTC stencil test --spec --coverage",
    "test:integ": "npm run test:cypress",
    "test:cypress": "cypress run",
    "test:cypress-dev": "cypress open --env disableSnapshotTests=true",
    "test:typescript": "tsc --noEmit",
    "test.watch": "TZ=UTC stencil test --spec --watchAll",
    "lint": "npm-run-all -p lint:eslint lint:stylelint",
    "fix": "npm run lint:fix",
    "lint:fix": "npm-run-all -p lint:fix-eslint lint:fix-stylelint",
    "lint:fix-eslint": "eslint --fix --ext .js,.ts,.tsx ./",
    "lint:fix-stylelint": "stylelint 'src/**/*.css' --fix",
    "lint:eslint": "eslint --ext .js,.ts,.tsx ./ --max-warnings 0",
    "lint:stylelint": "stylelint 'src/**/*.css' --max-warnings 0",
    "release": "npm run clean && npm-run-all -p build test lint",
    "prepublishOnly": "npm run release"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@stencil/core": "^1.12.2",
    "@stencil/router": "^1.0.1",
    "@testing-library/dom": "^7.22.2",
    "@testing-library/user-event": "^12.1.1",
    "@types/classnames": "^2.2.9",
    "@types/color": "^3.0.1",
    "@types/color-string": "^1.5.0",
    "@types/cypress-image-snapshot": "^3.1.5",
    "@types/d3": "^6.7.0",
    "@types/jest": "24.9.1",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isnumber": "^3.0.6",
    "@types/lodash.round": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/maplibre-gl": "^1.14.0",
    "@types/puppeteer": "^1.20.2",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "aws-sdk": "^2.943.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "color": "^3.1.2",
    "color-string": "^1.5.3",
    "copy-webpack-plugin": "^5.0.4",
    "cypress": "^6.9.1",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-shadow-dom": "^1.3.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^6.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "jest-extended": "^0.11.2",
    "jest-matcher-utils": "^24.9.0",
    "jest-raw-loader": "^1.0.1",
    "maplibre-gl": "^1.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "puppeteer": "^1.20.0",
    "query-string": "^6.13.5",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "dependencies": {
    "@stencil/react-output-target": "^0.1.0",
    "@stencil/redux": "^0.1.2",
    "@synchro-charts/core": "^1.0.0",
    "classnames": "^2.2.6",
    "d3-array": "^2.3.2",
    "detect-browser": "^5.0.0",
    "intervals-fn": "^3.0.3",
    "lodash.isequal": "^4.5.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.round": "^4.0.4",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "parse-duration": "^1.0.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "license": "Apache-2.0"
}
